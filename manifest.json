{
  "name": "تطبيق الترجمة الفورية",
  "short_name": "ترجمة فورية",
  "description": "تطبيق الترجمة الصوتية الفورية - تحدث بأي لغة واحصل على الترجمة فوراً",
  "start_url": "./index.html",
  "display": "standalone",
  "background_color": "#667eea",
  "theme_color": "#667eea",
  "orientation": "portrait-primary",
  "scope": "./",
  "lang": "ar",
  "dir": "rtl",
  "display_override": ["window-controls-overlay", "standalone"],
  "icons": [
    {
      "src": "logo-new.svg",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    },
    {
      "src": "logo-new.svg",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    }
  ],
  "categories": ["productivity", "utilities", "education"],
  "screenshots": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+translate",
      "url": "./translate.html?text=%s"
    }
  ],
  "shortcuts": [
    {
      "name": "ترجمة سريعة",
      "short_name": "ترجمة",
      "description": "فتح صفحة الترجمة مباشرة",
      "url": "./translate.html",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "192x192",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "الترجمة الذكية",
      "short_name": "ذكية",
      "description": "ترجمة النصوص والصور بالذكاء الاصطناعي",
      "url": "./smart-translate.html",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "192x192",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "الإعدادات",
      "short_name": "إعدادات",
      "description": "فتح صفحة الإعدادات",
      "url": "./settings.html",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "192x192",
          "type": "image/svg+xml"
        }
      ]
    }
  ],
  "file_handlers": [
    {
      "action": "./translate.html",
      "accept": {
        "text/plain": [".txt"]
      }
    }
  ],
  "share_target": {
    "action": "./translate.html",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  }
}